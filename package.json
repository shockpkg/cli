{
	"name": "@shockpkg/cli",
	"description": "shockpkg cli",
	"version": "1.5.0",
	"keywords": [
		"shockpkg",
		"package",
		"manager",
		"flash",
		"shockwave",
		"player",
		"air",
		"sdk"
	],
	"main": "lib/index",
	"exports": {
		"./module.mjs": "./module.mjs",
		".": [
			{
				"import": "./lib/index.mjs",
				"require": "./lib/index.js",
				"default": "./lib/index.js"
			},
			"./lib/index.js"
		]
	},
	"types": "lib/index.d.ts",
	"module": "module.mjs",
	"sideEffects": false,
	"bin": {
		"shockpkg": "./bin/run"
	},
	"engines": {
		"node": ">=10.0.0"
	},
	"scripts": {
		"clean": "gulp clean",
		"lint": "gulp lint",
		"build": "gulp build",
		"test": "gulp test",
		"all": "gulp all",
		"watch": "nodemon --exec 'gulp watched'",
		"prepack": "gulp prepack"
	},
	"repository": "https://github.com/shockpkg/cli.git",
	"bugs": "https://github.com/shockpkg/cli/issues",
	"author": "JrMasterModelBuilder",
	"copyright": "Copyright (c) 2018-2020 JrMasterModelBuilder",
	"license": "MPL-2.0",
	"dependencies": {
		"@babel/runtime": "^7.11.2",
		"@oclif/command": "^1.8.0",
		"@oclif/config": "^1.17.0",
		"@oclif/plugin-help": "^3.2.0",
		"@shockpkg/core": "^1.12.1",
		"source-map-support": "^0.5.19"
	},
	"devDependencies": {
		"@babel/core": "^7.11.6",
		"@babel/plugin-proposal-class-properties": "^7.10.4",
		"@babel/plugin-proposal-decorators": "^7.10.5",
		"@babel/plugin-transform-runtime": "^7.11.5",
		"@babel/preset-env": "^7.11.5",
		"@babel/preset-typescript": "^7.10.4",
		"@babel/register": "^7.11.5",
		"@oclif/dev-cli": "^1.22.2",
		"@oclif/test": "^1.2.7",
		"@types/jasmine": "^3.5.14",
		"@types/node": "^14.11.2",
		"@types/source-map-support": "^0.5.3",
		"@typescript-eslint/eslint-plugin": "^4.2.0",
		"@typescript-eslint/parser": "^4.2.0",
		"babel-plugin-esm-resolver": "^2.1.0",
		"chai": "^4.2.0",
		"del": "^5.1.0",
		"eslint": "^7.9.0",
		"eslint-plugin-import": "^2.22.0",
		"eslint-plugin-jsdoc": "^30.5.1",
		"execa": "^4.0.3",
		"gulp": "^4.0.2",
		"gulp-babel": "^8.0.0",
		"gulp-filter": "^6.0.0",
		"gulp-insert": "^0.5.0",
		"gulp-rename": "^2.0.0",
		"gulp-replace": "^1.0.0",
		"gulp-sourcemaps": "^2.6.5",
		"jasmine": "^3.6.1",
		"jasmine-core": "^3.6.0",
		"jasmine-spec-reporter": "^6.0.0",
		"nodemon": "^2.0.4",
		"ts-node": "^9.0.0",
		"typescript": "^4.0.3"
	},
	"files": [
		"/bin",
		"/lib",
		"/oclif.manifest.json",
		"/module.mjs"
	],
	"oclif": {
		"commands": "./lib/commands",
		"bin": "shockpkg",
		"plugins": [
			"@oclif/plugin-help"
		],
		"description": "shockpkg - shockpkg CLI\nCopyright (c) 2018-2020 JrMasterModelBuilder\nLicensed under the Mozilla Public License, v. 2.0"
	}
}
