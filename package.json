{
	"name": "@shockpkg/cli",
	"description": "shockpkg cli",
	"version": "2.0.3",
	"keywords": [
		"shockpkg",
		"package",
		"manager",
		"flash",
		"shockwave",
		"player",
		"air",
		"sdk"
	],
	"main": "cjs/index",
	"exports": {
		".": [
			{
				"import": "./esm/index.mjs",
				"require": "./cjs/index.js",
				"default": "./cjs/index.js"
			},
			"./cjs/index.js"
		]
	},
	"types": "dts/index.d.ts",
	"module": "esm/index.mjs",
	"sideEffects": false,
	"bin": {
		"shockpkg": "./bin/run"
	},
	"engines": {
		"node": ">=18.12.0"
	},
	"scripts": {
		"clean": "rimraf *.log* dts esm cjs oclif.manifest.json",
		"lint": "eslint .",
		"format": "prettier -w .",
		"formatted": "prettier -c .",
		"build:dts": "tsc",
		"build:esm": "babel --env-name esm -x .ts -s true -d esm --out-file-extension .mjs src",
		"build:cjs": "babel --env-name cjs -x .ts -s true -d cjs --out-file-extension .js src",
		"build:manifest": "oclif manifest",
		"build:readme": "oclif readme && node scripts/readme-cleanup.mjs",
		"build": "npm run build:dts && npm run build:esm && npm run build:cjs && npm run build:manifest && npm run build:readme",
		"test:esm": "node -r source-map-support/register --test esm",
		"test:cjs": "node -r source-map-support/register --test cjs",
		"test": "npm run test:esm && npm run test:cjs",
		"all:cjs": "npm run clean && npm run build:cjs && npm run test:cjs && npm run lint && npm run formatted",
		"all": "npm run clean && npm run build && npm run test && npm run lint && npm run formatted",
		"prepack": "npm run clean && npm run build"
	},
	"repository": "https://github.com/shockpkg/cli.git",
	"bugs": "https://github.com/shockpkg/cli/issues",
	"author": "JrMasterModelBuilder",
	"copyright": "Copyright (c) 2018-2023 JrMasterModelBuilder",
	"license": "MPL-2.0",
	"dependencies": {
		"@oclif/core": "^1.20.2",
		"@shockpkg/core": "^2.0.2",
		"source-map-support": "^0.5.21"
	},
	"devDependencies": {
		"@babel/cli": "^7.22.15",
		"@babel/core": "^7.19.6",
		"@babel/preset-env": "^7.19.4",
		"@babel/preset-typescript": "^7.18.6",
		"@types/node": "^18.11.9",
		"@types/source-map-support": "^0.5.6",
		"@typescript-eslint/eslint-plugin": "^5.42.0",
		"@typescript-eslint/parser": "^5.42.0",
		"babel-plugin-esm-resolver": "^2.3.0",
		"babel-plugin-search-and-replace": "^1.1.1",
		"eslint": "^8.26.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jsdoc": "^39.6.2",
		"oclif": "^3.2.24",
		"prettier": "^2.7.1",
		"rimraf": "^5.0.1",
		"typescript": "^4.8.4"
	},
	"files": [
		"/bin",
		"/dts",
		"!/dts/**/*.test.d.ts",
		"!/dts/**/*.spec.d.ts",
		"/esm",
		"!/esm/**/*.test.mjs",
		"!/esm/**/*.test.mjs.map",
		"!/esm/**/*.spec.mjs",
		"!/esm/**/*.spec.mjs.map",
		"/cjs",
		"!/cjs/**/*.test.js",
		"!/cjs/**/*.test.js.map",
		"!/cjs/**/*.spec.js",
		"!/cjs/**/*.spec.js.map",
		"/oclif.manifest.json"
	],
	"oclif": {
		"commands": "./cjs/commands",
		"bin": "shockpkg",
		"topicSeparator": ":",
		"plugins": [],
		"additionalHelpFlags": [
			"-h"
		],
		"additionalVersionFlags": [
			"-v"
		],
		"description": "shockpkg - The shockpkg CLI\nCopyright (c) 2018-2023 JrMasterModelBuilder\nLicensed under the Mozilla Public License, v. 2.0"
	}
}
