{
	"name": "@shockpkg/cli",
	"description": "shockpkg cli",
	"version": "2.0.1",
	"keywords": [
		"shockpkg",
		"package",
		"manager",
		"flash",
		"shockwave",
		"player",
		"air",
		"sdk"
	],
	"main": "lib/index",
	"exports": {
		"./module.mjs": "./module.mjs",
		".": [
			{
				"import": "./lib/index.mjs",
				"require": "./lib/index.js",
				"default": "./lib/index.js"
			},
			"./lib/index.js"
		]
	},
	"types": "lib/index.d.ts",
	"module": "module.mjs",
	"sideEffects": false,
	"bin": {
		"shockpkg": "./bin/run"
	},
	"engines": {
		"node": ">=14.15.0"
	},
	"scripts": {
		"clean": "gulp clean",
		"lint": "gulp lint",
		"format": "gulp format",
		"formatted": "gulp formatted",
		"build": "gulp build",
		"test": "gulp test",
		"all": "gulp all",
		"watch": "gulp watch",
		"prepack": "gulp prepack"
	},
	"repository": "https://github.com/shockpkg/cli.git",
	"bugs": "https://github.com/shockpkg/cli/issues",
	"author": "JrMasterModelBuilder",
	"copyright": "Copyright (c) 2018-2022 JrMasterModelBuilder",
	"license": "MPL-2.0",
	"dependencies": {
		"@oclif/core": "^1.18.0",
		"@shockpkg/core": "^2.0.1",
		"source-map-support": "^0.5.21"
	},
	"devDependencies": {
		"@babel/core": "^7.19.3",
		"@babel/preset-env": "^7.19.4",
		"@babel/preset-typescript": "^7.18.6",
		"@babel/register": "^7.18.9",
		"@oclif/test": "^2.2.3",
		"@types/gulp": "^4.0.9",
		"@types/gulp-babel": "^6.1.30",
		"@types/gulp-filter": "^3.0.34",
		"@types/gulp-insert": "^0.5.8",
		"@types/gulp-rename": "^2.0.1",
		"@types/gulp-replace": "0.0.31",
		"@types/gulp-sourcemaps": "0.0.35",
		"@types/jasmine": "^4.3.0",
		"@types/node": "^18.11.0",
		"@types/source-map-support": "^0.5.6",
		"@typescript-eslint/eslint-plugin": "^5.40.0",
		"@typescript-eslint/parser": "^5.40.0",
		"babel-plugin-esm-resolver": "^2.3.0",
		"chai": "^4.3.6",
		"eslint": "^8.25.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jsdoc": "^39.3.6",
		"gulp": "^4.0.2",
		"gulp-babel": "^8.0.0",
		"gulp-filter": "^7.0.0",
		"gulp-insert": "^0.5.0",
		"gulp-rename": "^2.0.0",
		"gulp-replace": "^1.1.3",
		"gulp-sourcemaps": "^3.0.0",
		"jasmine": "^4.4.0",
		"jasmine-core": "^4.4.0",
		"jasmine-spec-reporter": "^7.0.0",
		"oclif": "^3.2.16",
		"prettier": "^2.7.1",
		"ts-node": "^10.9.1",
		"typescript": "^4.8.4"
	},
	"files": [
		"/bin",
		"/lib",
		"/oclif.manifest.json",
		"/module.mjs"
	],
	"oclif": {
		"commands": "./lib/commands",
		"bin": "shockpkg",
		"topicSeparator": ":",
		"plugins": [],
		"additionalHelpFlags": [
			"-h"
		],
		"additionalVersionFlags": [
			"-v"
		],
		"description": "shockpkg - The shockpkg CLI\nCopyright (c) 2018-2022 JrMasterModelBuilder\nLicensed under the Mozilla Public License, v. 2.0"
	}
}
